# Databricks OAuth OIDC 設定 (例)
# 実際の環境に合わせて値を変更してください

# Databricks環境設定
DATABRICKS_HOST=adb-xxxxxxxxx.xx.azuredatabricks.net
DATABRICKS_CLIENT_ID=your-oauth-client-id
DATABRICKS_CLIENT_SECRET=your-oauth-client-secret
DATABRICKS_SERVING_ENDPOINT=your-model-serving-endpoint-name

# アプリケーション設定
PORT=3000
NODE_ENV=production
SESSION_SECRET=your-secure-session-secret

# OAuth設定
REDIRECT_URI=https://your-company-domain.com/oauth/callback

# 開発環境での例 (localtunnel使用時)
# REDIRECT_URI=https://your-app-name.loca.lt/oauth/callback